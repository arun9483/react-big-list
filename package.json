{
  "name": "react-big-list",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "remove-api-client": "rimraf ./src/api-client/src",
    "pregenerate-api-client": "npm run remove-api-client",
    "generate-api-client": "openapi-generator-cli generate -i src/api-client/spec.json -g typescript-fetch -o src/api-client/src"
  },
  "dependencies": {
    "@types/react-world-flags": "^1.4.5",
    "react": "^18.2.0",
    "react-country-flag": "^3.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/node": "^20.13.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "rimraf": "^5.0.7",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
